# Development environment variables
# This file is only loaded in development mode

# Disable HMR refresh warnings
NEXT_PUBLIC_DISABLE_HMR_WARNINGS=true

# Enable React DevTools
NEXT_PUBLIC_ENABLE_REACT_DEVTOOLS=true

# Development-specific API URLs
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Office service URL
NEXT_PUBLIC_OFFICE_SERVICE_URL=http://localhost:8002

# Enable debug mode
NEXT_PUBLIC_DEBUG=true